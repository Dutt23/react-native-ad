{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "quotes": [
            "error",
            "double"
        ],
        "semi": ["error", "always"]
    }
}
